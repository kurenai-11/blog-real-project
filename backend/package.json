{
  "name": "backend",
  "version": "0.0.0",
  "description": "",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently npm:watch-ts npm:mongod npm:nodemon",
    "start": "node dist/app.js",
    "preview": "npm run build && concurrently npm:mongod npm:node",
    "build": "rm -rf dist/ && mkdir dist/ && swc ./src -d dist/ -s false --ignore **/*.test.js",
    "watch-ts": "swc src/ --ignore **/*.test.ts -d dist/ -q -s true -w",
    "mongod": "mongod > /dev/null",
    "nodemon": "nodemon -q -w dist/ .",
    "node": "node ."
  },
  "keywords": [],
  "author": "kurenai-11",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.25",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^6.8.2",
    "zod": "^3.20.2"
  }
}
