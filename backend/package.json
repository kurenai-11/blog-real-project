  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently npm:watch-ts npm:mongod npm:nodemon",
    "preview": "npm run build && concurrently npm:mongod npm:node",
    "build": "rm -rf dist/ && mkdir dist/ && swc ./src -d dist/ -s false",
    "watch-ts": "swc src/ -d dist/ -q -s true -w",
